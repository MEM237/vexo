{
  "manifest_version": 3,
  "name": "VEXO Escalator",
  "version": "1.0",
  "description": "A Chrome extension for video and text communication.",
  "permissions": ["storage", "scripting", "activeTab", "notifications"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Open VEXO",
    "default_popup": "src/html/popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["placeholder0.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}
