{
  "manifest_version": 3,
  "name": "VEXO Escalator",
  "version": "1.0",
  "description": "A Chrome extension for video and text communication.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "src/js/background.js"
  },
  "action": {
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/Icon-togel.png",
        "assets/icons/Icon-cake.png",
        "assets/icons/Icon-explore.png",
        "assets/icons/Icon-heart-broken.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
